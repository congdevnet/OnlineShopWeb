    <script src="~/Areas/plugins/pdfmake/pdfmake.min.js"></script>
    <script src="~/Areas/plugins/pdfmake/vfs_fonts.js"></script>
    <script src="~/Areas/plugins/datatables-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Areas/plugins/datatables-buttons/js/buttons.print.min.js"></script>
    <script src="~/Areas/plugins/datatables-buttons/js/buttons.colVis.min.js"></script>
    <script src="~/Areas/plugins/sweetalert2/sweetalert2.min.js"></script>
    <script src="~/Areas/plugins/toastr/toastr.min.js"></script>
    <!-- Select2 -->
    <script src="~/Areas/plugins/select2/js/select2.full.min.js"></script>
    <!-- Bootstrap4 Duallistbox -->
    <script src="~/Areas/plugins/bootstrap4-duallistbox/jquery.bootstrap-duallistbox.min.js"></script>
    <!-- InputMask -->
    <script src="~/Areas/plugins/moment/moment.min.js"></script>
    <script src="~/Areas/plugins/inputmask/jquery.inputmask.min.js"></script>
    <!-- date-range-picker -->
    <script src="~/Areas/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- bootstrap color picker -->
    <script src="~/Areas/plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/Areas/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Bootstrap Switch -->
    <script src="~/Areas/plugins/bootstrap-switch/js/bootstrap-switch.min.js"></script>
    <!-- BS-Stepper -->
    <script src="~/Areas/plugins/bs-stepper/js/bs-stepper.min.js"></script>
    <!-- dropzonejs -->
    <script src="~/Areas/plugins/dropzone/min/dropzone.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/asset/ckfinder/ckfinder.js"></script>
    <script src="~/Areas/dist/js/adminlte.min.js"></script>
    <script src="~/Areas/hethong.js"></script>
    <script src="~/asset/ckeditor/ckeditor.js"></script>
    <!-- Page specific script -->
    @RenderSection("scripts", required: false)
    <script>
        function dangxuat() {
             $.ajax({
                url: '@Url.Action("Loguot", "Login")',
                dataType: "json",
                type: "GET",
                async: true,
                processData: false,
                cache: false,
                success: function (data) {
                    if (data.Status == 200) {
                        window.location.href = "@Url.Action("Index", "Login")"
                    }
                },
                error: function (xhr) {
                    alert(xhr);
                }
            });
        }
    </script>
</body>
</html>
